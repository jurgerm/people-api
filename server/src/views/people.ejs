<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("partials/head", {title: "People"}) %> 
</head>
<body>
    <main>
        <% people.forEach(person => { %>
            <%- include("components/card", {
                title: `${person.name} ${person.lastname}`,
                content: `This person is ${person.age} years old`,
                titleLinkTo: `/views/person/${person.id}`
            }) %> 
        <% }) %>
    </main>
</body>
</html>